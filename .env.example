# CI2P Laboratory MedDef App Configuration Template
# Copy this file to .env and update with your actual values

# VPS Server Configuration
CI2P_SERVER_HOST=your.server.ip.address
CI2P_SERVER_PORT=8000
CI2P_SERVER_PROTOCOL=http
CI2P_MODEL_BASE_PATH=/path/to/models

# App Configuration
APP_NAME=MedDef
APP_VERSION=1.0.0
APP_ENVIRONMENT=development

# Laboratory Information
LAB_NAME=Your Laboratory Name
INSTITUTION=Your Institution Name
INSTITUTION_SHORT=Your University

# Model Download Configuration
MAX_DOWNLOAD_RETRIES=3
DOWNLOAD_TIMEOUT_MS=30000
CACHE_EXPIRY_DAYS=7

# Development/Debug Settings
DEBUG_MODE=true
ENABLE_MOCK_MODELS=false
LOG_LEVEL=info

# Future API Keys (replace with actual keys when needed)
# OPENAI_API_KEY=your_openai_key_here
# HUGGINGFACE_API_KEY=your_hf_key_here
# TENSORFLOWJS_CDN_KEY=your_tf_cdn_key_here

# Analytics Configuration
# ANALYTICS_ENABLED=false
# FIREBASE_PROJECT_ID=your_firebase_project
# SENTRY_DSN=your_sentry_dsn

# Security Configuration
REQUIRE_AUTH=false
API_RATE_LIMIT=100
# JWT_SECRET=your_jwt_secret_here
